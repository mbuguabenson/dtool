<xml xmlns="https://developers.google.com/blockly/xml" is_dbot="true" collection="false">
  <variables>
    <variable id="stake">Stake</variable>
    <variable id="multiplier">Multiplier</variable>
    <variable id="current_stake">Current Stake</variable>
    <variable id="consecutive_losses">Consecutive Losses</variable>
    <variable id="market_index">Market Index</variable>
    <variable id="markets">Markets</variable>
    <variable id="digit_list">Digit List</variable>
    <variable id="streak_even">Streak Even</variable>
    <variable id="streak_odd">Streak Odd</variable>
    <variable id="power_even">Even Power</variable>
    <variable id="power_odd">Odd Power</variable>
    <variable id="count_even">Count Even</variable>
    <variable id="i">i</variable>
    <variable id="text">text</variable>
    <variable id="recovery_mode">Recovery Mode</variable>
    <variable id="alternate">Alternate</variable>
  </variables>
  <block type="trade_definition" id="trade_def" deletable="false" x="0" y="0">
    <statement name="TRADE_OPTIONS">
      <block type="trade_definition_market" id="market">
        <field name="MARKET_LIST">synthetic_index</field>
        <field name="SUBMARKET_LIST">random_index</field>
        <value name="SYMBOL_LIST">
          <block type="lists_getIndex">
            <mutation statement="false" at="true"></mutation>
            <field name="MODE">GET</field>
            <field name="WHERE">FROM_START</field>
            <value name="VALUE"><block type="variables_get"><field name="VAR" id="markets">Markets</field></block></value>
            <value name="AT"><block type="variables_get"><field name="VAR" id="market_index">Market Index</field></block></value>
          </block>
        </value>
        <next>
          <block type="trade_definition_tradetype" id="trade_type">
            <field name="TRADETYPECAT_LIST">digits</field>
            <field name="TRADETYPE_LIST">evenodd</field>
            <next>
              <block type="trade_definition_contracttype" id="contract_type">
                <field name="TYPE_LIST">both</field>
                <next>
                  <block type="trade_definition_candleinterval" id="interval">
                    <field name="CANDLEINTERVAL_LIST">60</field>
                    <next>
                      <block type="trade_definition_restartbuysell" id="restart">
                        <field name="TIME_MACHINE_ENABLED">FALSE</field>
                        <next>
                          <block type="trade_definition_restartonerror" id="onerror">
                            <field name="RESTARTONERROR">TRUE</field>
                          </block>
                        </next>
                      </block>
                    </next>
                  </block>
                </next>
              </block>
            </next>
          </block>
        </next>
      </block>
    </statement>
    <statement name="INITIALIZATION">
      <block type="variables_set" id="init_markets">
        <field name="VAR" id="markets">Markets</field>
        <value name="VALUE">
          <block type="lists_create_with">
            <mutation items="5"></mutation>
            <value name="ADD0"><block type="text"><field name="TEXT">1HZ10V</field></block></value>
            <value name="ADD1"><block type="text"><field name="TEXT">1HZ25V</field></block></value>
            <value name="ADD2"><block type="text"><field name="TEXT">1HZ50V</field></block></value>
            <value name="ADD3"><block type="text"><field name="TEXT">1HZ75V</field></block></value>
            <value name="ADD4"><block type="text"><field name="TEXT">1HZ100V</field></block></value>
          </block>
        </value>
        <next>
          <block type="variables_set" id="init_mi">
            <field name="VAR" id="market_index">Market Index</field>
            <value name="VALUE"><block type="math_number"><field name="NUM">1</field></block></value>
            <next>
              <block type="variables_set" id="init_stake">
                <field name="VAR" id="stake">Stake</field>
                <value name="VALUE"><block type="math_number"><field name="NUM">0.35</field></block></value>
                <next>
                  <block type="variables_set" id="init_multiplier">
                    <field name="VAR" id="multiplier">Multiplier</field>
                    <value name="VALUE"><block type="math_number"><field name="NUM">2.1</field></block></value>
                    <next>
                      <block type="variables_set" id="init_cs">
                        <field name="VAR" id="current_stake">Current Stake</field>
                        <value name="VALUE"><block type="variables_get"><field name="VAR" id="stake">Stake</field></block></value>
                        <next>
                          <block type="variables_set" id="init_cl">
                            <field name="VAR" id="consecutive_losses">Consecutive Losses</field>
                            <value name="VALUE"><block type="math_number"><field name="NUM">0</field></block></value>
                            <next>
                              <block type="variables_set" id="init_rm">
                                <field name="VAR" id="recovery_mode">Recovery Mode</field>
                                <value name="VALUE"><block type="logic_boolean"><field name="BOOL">FALSE</field></block></value>
                              </block>
                            </next>
                          </block>
                        </next>
                      </block>
                    </next>
                  </block>
                </next>
              </block>
            </next>
          </block>
        </next>
      </block>
    </statement>
    <statement name="SUBMARKET">
      <block type="trade_definition_tradeoptions" id="options">
        <mutation has_first_barrier="false" has_second_barrier="false" has_prediction="true"></mutation>
        <field name="DURATIONTYPE_LIST">t</field>
        <value name="DURATION"><block type="math_number"><field name="NUM">1</field></block></value>
        <value name="AMOUNT"><block type="variables_get"><field name="VAR" id="current_stake">Current Stake</field></block></value>
        <value name="PREDICTION">
          <block type="logic_ternary">
            <value name="IF"><block type="logic_compare"><field name="OP">EQ</field><value name="A"><block type="variables_get"><field name="VAR" id="alternate">Alternate</field></block></value><value name="B"><block type="math_number"><field name="NUM">0</field></block></value></block></value>
            <value name="THEN"><block type="math_number"><field name="NUM">2</field></block></value>
            <value name="ELSE"><block type="math_number"><field name="NUM">8</field></block></value>
          </block>
        </value>
      </block>
    </statement>
  </block>
  <block type="before_purchase" id="before" x="0" y="1000">
    <statement name="BEFOREPURCHASE_STACK">
      <block type="variables_set" id="get_list">
        <field name="VAR" id="digit_list">Digit List</field>
        <value name="VALUE"><block type="lastDigitList"></block></value>
        <next>
          <block type="variables_set" id="reset_c_even">
            <field name="VAR" id="count_even">Count Even</field>
            <value name="VALUE"><block type="math_number"><field name="NUM">0</field></block></value>
            <next>
              <block type="controls_for" id="power_loop">
                <field name="VAR" id="i">i</field>
                <value name="FROM"><block type="math_number"><field name="NUM">1</field></block></value>
                <value name="TO"><block type="math_number"><field name="NUM">100</field></block></value>
                <value name="BY"><block type="math_number"><field name="NUM">1</field></block></value>
                <statement name="DO">
                  <block type="controls_if" id="check_p_even">
                    <value name="IF0">
                      <block type="math_number_property"><mutation divisor_input="false"></mutation><field name="PROPERTY">EVEN</field><value name="NUMBER_TO_CHECK"><block type="lists_getIndex"><mutation statement="false" at="true"></mutation><field name="MODE">GET</field><field name="WHERE">FROM_START</field><value name="VALUE"><block type="variables_get"><field name="VAR" id="digit_list">Digit List</field></block></value><value name="AT"><block type="variables_get"><field name="VAR" id="i">i</field></block></value></block></value></block>
                    </value>
                    <statement name="DO0">
                      <block type="math_change" id="inc_p_even"><field name="VAR" id="count_even">Count Even</field><value name="DELTA"><block type="math_number"><field name="NUM">1</field></block></value></block>
                    </statement>
                  </block>
                </statement>
                <next>
                  <block type="variables_set" id="set_p_even">
                    <field name="VAR" id="power_even">Even Power</field>
                    <value name="VALUE"><block type="variables_get"><field name="VAR" id="count_even">Count Even</field></block></value>
                    <next>
                      <block type="variables_set" id="set_p_odd">
                        <field name="VAR" id="power_odd">Odd Power</field>
                        <value name="VALUE"><block type="math_arithmetic"><field name="OP">MINUS</field><value name="A"><block type="math_number"><field name="NUM">100</field></block></value><value name="B"><block type="variables_get"><field name="VAR" id="power_even">Even Power</field></block></value></block></value>
                        <next>
                          <block type="notify" id="notif">
                            <field name="NOTIFICATION_TYPE">info</field>
                            <field name="NOTIFICATION_SOUND">silent</field>
                            <value name="MESSAGE">
                              <block type="text_join">
                                <mutation items="3"></mutation>
                                <value name="ADD0"><block type="text"><field name="TEXT">Scanning </field></block></value>
                                <value name="ADD1">
                                  <block type="lists_getIndex">
                                    <mutation statement="false" at="true"></mutation>
                                    <field name="MODE">GET</field>
                                    <field name="WHERE">FROM_START</field>
                                    <value name="VALUE"><block type="variables_get"><field name="VAR" id="markets">Markets</field></block></value>
                                    <value name="AT"><block type="variables_get"><field name="VAR" id="market_index">Market Index</field></block></value>
                                  </block>
                                </value>
                                <value name="ADD2">
                                  <block type="text_join">
                                    <mutation items="2"></mutation>
                                    <value name="ADD0"><block type="text"><field name="TEXT"> | EP: </field></block></value>
                                    <value name="ADD1"><block type="variables_get"><field name="VAR" id="power_even">Even Power</field></block></value>
                                  </block>
                                </value>
                              </block>
                            </value>
                            <next>
                              <block type="controls_if" id="main_logic">
                                <mutation else="1"></mutation>
                                <value name="IF0"><block type="variables_get"><field name="VAR" id="recovery_mode">Recovery Mode</field></block></value>
                                <statement name="DO0">
                                  <block type="controls_if" id="rec_buy">
                                    <mutation else="1"></mutation>
                                    <value name="IF0"><block type="logic_compare"><field name="OP">EQ</field><value name="A"><block type="variables_get"><field name="VAR" id="alternate">Alternate</field></block></value><value name="B"><block type="math_number"><field name="NUM">0</field></block></value></block></value>
                                    <statement name="DO0"><block type="purchase" id="buy_over"><field name="PURCHASE_LIST">DIGITOVER</field><field name="ALLOW_BULK_PURCHASE">no</field><field name="NUM_TRADES">1</field></block></statement>
                                    <statement name="ELSE"><block type="purchase" id="buy_under"><field name="PURCHASE_LIST">DIGITUNDER</field><field name="ALLOW_BULK_PURCHASE">no</field><field name="NUM_TRADES">1</field></block></statement>
                                  </block>
                                </statement>
                                <statement name="ELSE">
                                  <block type="controls_if" id="entry_check">
                                    <mutation elseif="1" else="1"></mutation>
                                    <value name="IF0">
                                      <block type="logic_operation" inline="false">
                                        <field name="OP">AND</field>
                                        <value name="A"><block type="logic_compare"><field name="OP">GT</field><value name="A"><block type="variables_get"><field name="VAR" id="power_even">Even Power</field></block></value><value name="B"><block type="math_number"><field name="NUM">55</field></block></value></block></value>
                                        <value name="B">
                                          <block type="logic_operation" inline="false">
                                            <field name="OP">AND</field>
                                            <value name="A"><block type="math_number_property"><mutation divisor_input="false"></mutation><field name="PROPERTY">EVEN</field><value name="NUMBER_TO_CHECK"><block type="lists_getIndex"><mutation statement="false" at="true"></mutation><field name="MODE">GET</field><field name="WHERE">FROM_START</field><value name="VALUE"><block type="variables_get"><field name="VAR" id="digit_list">Digit List</field></block></value><value name="AT"><block type="math_number"><field name="NUM">1</field></block></value></block></value></block></value>
                                            <value name="B">
                                              <block type="logic_operation" inline="false">
                                                <field name="OP">AND</field>
                                                <value name="A"><block type="math_number_property"><mutation divisor_input="false"></mutation><field name="PROPERTY">ODD</field><value name="NUMBER_TO_CHECK"><block type="lists_getIndex"><mutation statement="false" at="true"></mutation><field name="MODE">GET</field><field name="WHERE">FROM_START</field><value name="VALUE"><block type="variables_get"><field name="VAR" id="digit_list">Digit List</field></block></value><value name="AT"><block type="math_number"><field name="NUM">2</field></block></value></block></value></block></value>
                                                <value name="B"><block type="math_number_property"><mutation divisor_input="false"></mutation><field name="PROPERTY">ODD</field><value name="NUMBER_TO_CHECK"><block type="lists_getIndex"><mutation statement="false" at="true"></mutation><field name="MODE">GET</field><field name="WHERE">FROM_START</field><value name="VALUE"><block type="variables_get"><field name="VAR" id="digit_list">Digit List</field></block></value><value name="AT"><block type="math_number"><field name="NUM">3</field></block></value></block></value></block></value>
                                              </block>
                                            </value>
                                          </block>
                                        </value>
                                      </block>
                                    </value>
                                    <statement name="DO0"><block type="purchase" id="buy_even"><field name="PURCHASE_LIST">DIGITEVEN</field><field name="ALLOW_BULK_PURCHASE">no</field><field name="NUM_TRADES">1</field></block></statement>
                                    <value name="IF1">
                                      <block type="logic_operation" inline="false">
                                        <field name="OP">AND</field>
                                        <value name="A"><block type="logic_compare"><field name="OP">GT</field><value name="A"><block type="variables_get"><field name="VAR" id="power_odd">Odd Power</field></block></value><value name="B"><block type="math_number"><field name="NUM">55</field></block></value></block></value>
                                        <value name="B">
                                          <block type="logic_operation" inline="false">
                                            <field name="OP">AND</field>
                                            <value name="A"><block type="math_number_property"><mutation divisor_input="false"></mutation><field name="PROPERTY">ODD</field><value name="NUMBER_TO_CHECK"><block type="lists_getIndex"><mutation statement="false" at="true"></mutation><field name="MODE">GET</field><field name="WHERE">FROM_START</field><value name="VALUE"><block type="variables_get"><field name="VAR" id="digit_list">Digit List</field></block></value><value name="AT"><block type="math_number"><field name="NUM">1</field></block></value></block></value></block></value>
                                            <value name="B">
                                              <block type="logic_operation" inline="false">
                                                <field name="OP">AND</field>
                                                <value name="A"><block type="math_number_property"><mutation divisor_input="false"></mutation><field name="PROPERTY">EVEN</field><value name="NUMBER_TO_CHECK"><block type="lists_getIndex"><mutation statement="false" at="true"></mutation><field name="MODE">GET</field><field name="WHERE">FROM_START</field><value name="VALUE"><block type="variables_get"><field name="VAR" id="digit_list">Digit List</field></block></value><value name="AT"><block type="math_number"><field name="NUM">2</field></block></value></block></value></block></value>
                                                <value name="B"><block type="math_number_property"><mutation divisor_input="false"></mutation><field name="PROPERTY">EVEN</field><value name="NUMBER_TO_CHECK"><block type="lists_getIndex"><mutation statement="false" at="true"></mutation><field name="MODE">GET</field><field name="WHERE">FROM_START</field><value name="VALUE"><block type="variables_get"><field name="VAR" id="digit_list">Digit List</field></block></value><value name="AT"><block type="math_number"><field name="NUM">3</field></block></value></block></value></block></value>
                                              </block>
                                            </value>
                                          </block>
                                        </value>
                                      </block>
                                    </value>
                                    <statement name="DO1"><block type="purchase" id="buy_odd"><field name="PURCHASE_LIST">DIGITODD</field><field name="ALLOW_BULK_PURCHASE">no</field><field name="NUM_TRADES">1</field></block></statement>
                                    <statement name="ELSE">
                                       <block type="math_change" id="inc_m">
                                          <field name="VAR" id="market_index">Market Index</field>
                                          <value name="DELTA"><block type="math_number"><field name="NUM">1</field></block></value>
                                          <next>
                                            <block type="controls_if" id="check_m_limit">
                                              <value name="IF0"><block type="logic_compare"><field name="OP">GT</field><value name="A"><block type="variables_get"><field name="VAR" id="market_index">Market Index</field></block></value><value name="B"><block type="math_number"><field name="NUM">5</field></block></value></block></value>
                                              <statement name="DO0"><block type="variables_set" id="reset_m"><field name="VAR" id="market_index">Market Index</field><value name="VALUE"><block type="math_number"><field name="NUM">1</field></block></value></block></statement>
                                            </block>
                                          </next>
                                       </block>
                                    </statement>
                                  </block>
                                </statement>
                              </block>
                            </next>
                          </block>
                        </next>
                      </block>
                    </next>
                  </block>
                </next>
              </block>
            </next>
          </block>
        </next>
      </block>
    </statement>
  </block>
  <block type="after_purchase" id="after" x="0" y="2400">
    <statement name="AFTERPURCHASE_STACK">
      <block type="controls_if" id="final">
        <mutation else="1"></mutation>
        <value name="IF0"><block type="contract_check_result"><field name="CHECK_RESULT">win</field></block></value>
        <statement name="DO0">
          <block type="variables_set" id="win_res">
            <field name="VAR" id="current_stake">Current Stake</field>
            <value name="VALUE"><block type="variables_get"><field name="VAR" id="stake">Stake</field></block></value>
            <next>
              <block type="variables_set" id="cl_res">
                <field name="VAR" id="consecutive_losses">Consecutive Losses</field>
                <value name="VALUE"><block type="math_number"><field name="NUM">0</field></block></value>
                <next>
                  <block type="variables_set" id="rm_res">
                    <field name="VAR" id="recovery_mode">Recovery Mode</field>
                    <value name="VALUE"><block type="logic_boolean"><field name="BOOL">FALSE</field></block></value>
                  </block>
                </next>
              </block>
            </next>
          </block>
        </statement>
        <statement name="ELSE">
          <block type="math_change" id="inc_cl">
            <field name="VAR" id="consecutive_losses">Consecutive Losses</field>
            <value name="DELTA"><block type="math_number"><field name="NUM">1</field></block></value>
            <next>
              <block type="variables_set" id="mart">
                <field name="VAR" id="current_stake">Current Stake</field>
                <value name="VALUE">
                  <block type="math_arithmetic">
                    <field name="OP">MULTIPLY</field>
                    <value name="A"><block type="variables_get"><field name="VAR" id="current_stake">Current Stake</field></block></value>
                    <value name="B"><block type="variables_get"><field name="VAR" id="multiplier">Multiplier</field></block></value>
                  </block>
                </value>
                <next>
                  <block type="variables_set" id="toggle_alt">
                    <field name="VAR" id="alternate">Alternate</field>
                    <value name="VALUE">
                      <block type="logic_ternary">
                        <value name="IF"><block type="logic_compare"><field name="OP">EQ</field><value name="A"><block type="variables_get"><field name="VAR" id="alternate">Alternate</field></block></value><value name="B"><block type="math_number"><field name="NUM">0</field></block></value></block></value>
                        <value name="THEN"><block type="math_number"><field name="NUM">1</field></block></value>
                        <value name="ELSE"><block type="math_number"><field name="NUM">0</field></block></value>
                      </block>
                    </value>
                    <next>
                      <block type="controls_if" id="check_cl">
                        <value name="IF0"><block type="logic_compare"><field name="OP">GTE</field><value name="A"><block type="variables_get"><field name="VAR" id="consecutive_losses">Consecutive Losses</field></block></value><value name="B"><block type="math_number"><field name="NUM">4</field></block></value></block></value>
                        <statement name="DO0">
                          <block type="variables_set" id="set_rm"><field name="VAR" id="recovery_mode">Recovery Mode</field><value name="VALUE"><block type="logic_boolean"><field name="BOOL">TRUE</field></block></value></block>
                        </statement>
                      </block>
                    </next>
                  </block>
                </next>
              </block>
            </next>
          </block>
        </statement>
        <next><block type="trade_again"></block></next>
      </block>
    </statement>
  </block>
</xml>
