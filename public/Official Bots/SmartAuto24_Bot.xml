<xml xmlns="http://www.w3.org/1999/xhtml" collection="false">
  <block type="trade_definition" id="smartauto24-bot-root" x="0" y="0">
    <statement name="TRADE_OPTIONS">
      <block type="trade_definition_market" id="market-select" deletable="false" movable="false">
        <field name="MARKET_LIST">synthetic_index</field>
        <field name="SUBMARKET_LIST">random_index</field>
        <field name="SYMBOL_LIST">R_100</field>
        <next>
          <block type="trade_definition_tradetype" id="type-select" deletable="false" movable="false">
            <field name="TRADETYPECAT_LIST">digits</field>
            <field name="TRADETYPE_LIST">overunder</field>
            <next>
              <block type="trade_definition_contracttype" id="contract-select" deletable="false" movable="false">
                <field name="TYPE_LIST">both</field>
                <next>
                  <block type="trade_definition_candleinterval" id="interval-select" deletable="false" movable="false">
                    <field name="CANDLEINTERVAL_LIST">60</field>
                    <next>
                      <block type="trade_definition_restartbuysell" id="restart-select" deletable="false" movable="false">
                        <field name="TIME_MACHINE_ENABLED">FALSE</field>
                        <next>
                          <block type="trade_definition_restartonerror" id="onerror-select" deletable="false" movable="false">
                            <field name="RESTARTONERROR">TRUE</field>
                          </block>
                        </next>
                      </block>
                    </next>
                  </block>
                </next>
              </block>
            </next>
          </block>
        </next>
      </block>
    </statement>
    <statement name="INITIALIZATION">
      <block type="variables_set" id="init-stake">
        <field name="VAR">stake</field>
        <value name="VALUE">
          <block type="math_number">
            <field name="NUM">0.35</field>
          </block>
        </value>
      </block>
    </statement>
    <statement name="SUBMISSION">
      <block type="trade_definition_tradeoptions" id="options">
        <field name="DURATIONTYPE_LIST">t</field>
        <value name="DURATION">
          <block type="math_number">
            <field name="NUM">1</field>
          </block>
        </value>
        <value name="AMOUNT">
          <block type="variables_get">
            <field name="VAR">stake</field>
          </block>
        </value>
        <value name="PREDICTION">
          <block type="math_number">
            <field name="NUM">2</field>
          </block>
        </value>
      </block>
    </statement>
  </block>
  <block type="before_purchase" id="strategy" x="0" y="450">
    <statement name="BEFOREPURCHASE_STACK">
      <block type="controls_if" id="check-power">
        <value name="IF0">
          <block type="logic_compare" id="compare-power">
            <field name="OP">GTE</field>
            <value name="A">
              <block type="analysis_get_power" id="get-over-power">
                <field name="TARGET">Over</field>
              </block>
            </value>
            <value name="B">
              <block type="math_number">
                <field name="NUM">55</field>
              </block>
            </value>
          </block>
        </value>
        <statement name="DO0">
          <block type="purchase" id="buy-over">
            <field name="PURCHASE_LIST">DIGITOVER</field>
          </block>
        </statement>
      </block>
    </statement>
  </block>
  <block type="after_purchase" id="finish" x="0" y="650">
    <statement name="AFTERPURCHASE_STACK">
      <block type="controls_if" id="check-result">
        <mutation else="1"></mutation>
        <value name="IF0">
          <block type="contract_check_result" id="win-check">
            <field name="CHECK_RESULT">win</field>
          </block>
        </value>
        <statement name="DO0">
          <block type="notify" id="win-notify">
            <field name="NOTIFICATION_TYPE">success</field>
            <field name="NOTIFICATION_SOUND">silent</field>
            <value name="MESSAGE">
              <block type="text">
                <field name="TEXT">Win!</field>
              </block>
            </value>
          </block>
        </statement>
        <statement name="ELSE">
          <block type="math_change" id="martingale">
            <field name="VAR">stake</field>
            <value name="DELTA">
              <block type="math_arithmetic">
                <field name="OP">MULTIPLY</field>
                <value name="A">
                  <block type="variables_get">
                    <field name="VAR">stake</field>
                  </block>
                </value>
                <value name="B">
                  <block type="math_number">
                    <field name="NUM">1.1</field>
                  </block>
                </value>
              </block>
            </value>
          </block>
        </statement>
      </block>
      <block type="trade_again" id="again"></block>
    </statement>
  </block>
</xml>
